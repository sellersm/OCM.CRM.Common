<TableSpec 
  Tablename="USR_PHOTOINTERACTION_EXCEPTIONS" 
  Author="Memphis Sellers" 
  Description="Holds the exceptions to process that completes Annual Photo interactions." 
  Name="Photo Interaction Exceptions" 
  ID="1d0744c1-d51c-4590-92a1-ba2768c472ea" 
  xmlns:common="bb_appfx_commontypes" 
  xmlns="bb_appfx_table">
  <!-- Remarks: Table holds the exceptions generated by the process that completes annual photo interactions, and unusable annual photo interactions. 
  EXCEPTION column can have these values: 
  "Annual photo already completed", 
  "Interaction Not Found", 
  "Unusable photo already completed" 
  
  13-Mar-2014	Memphis		Added the COMPLETEDDATE column to keep track of when the interaction was completed, if it was.
  -->

  <!-- define fields on the table -->
  <Fields>
	<GuidField Description="Interaction Id" Name="INTERACTIONID" Required="false"/>
	<TextField Description="Child lookup Id" Name="CHILDLOOKUPID" Required="true" Length="10"/>
	<TextField Description="Exception" Name="EXCEPTION" Required="true" Length="100"/>
	<DateField Name="ACTUALDATE" Required="true"/>
	<DateField Name="COMPLETEDDATE" Required="false" Description="Date interaction completed"/>
	<GuidField Description="Unique Id of complete interaction" Name="COMPLETEINTERACTIONPROCESSID" Required="false"/>
  </Fields>
  <!-- define any additional check constraints on the table -->

  <!--<CheckConstraints> </CheckConstraints>-->

  <!-- define any indexes on the table -->

  <!--<Indexes> </Indexes>-->

  <!-- define any triggers on the table -->

  <!--<Triggers> </Triggers>-->
</TableSpec>