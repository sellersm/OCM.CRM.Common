	Description="Holds the log generated by rate increase implementation script."
	Author="Memphis Sellers"
	CustomIdentifier="true"
	Tablename="USR_RATECHANGE_IMPLEMENTATION_LOGPROCESS"
	>

	<!-- Remarks:  Table holds the log/process generated by the process that implements the rate increase change,
	-->

	<!-- define fields on the table -->
	<Fields>
		<GuidField Name="SESSIONID" Description="Session Id" Required="true"/>
		<DateField Name="IMPLMENTATIONDATE" Description="Date of implementation" Required="true"/>
		<GuidField Name="REVENUEID" Description="Revenue Id" Required="false"/>
		<TextField Name="LOGMESSAGE" Length="1000" Description="Message" Required="false"/>
		<MoneyField Name="GIFTAMOUNT" Description="New gift amount" Required="false"/>
		<BooleanField Name="RECORDCOMPLETED" Description="This record is done" />
	</Fields>

